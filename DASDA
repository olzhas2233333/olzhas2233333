	Name = "MARAT LOX",
	Content = "JDI SWAT DETKA",
	Image = "rbxassetid://4483345998",
	Time = 5
})


local Tab = Window:MakeTab({
	Name = "MARAT LOX",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "MARAT LOX"
})

Tab:AddButton({
	Name = "BOMBA!",
	Callback = function()
      		print("BOMBA")
  	end    
})

Tab:AddToggle({
	Name = "This is a MARAT!",
	Default = false,
	Callback = function(Value)
		print("LOX")
	end    
})

end
OrionLib:Init()
